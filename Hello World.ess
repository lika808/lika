global _start

section .data
    message db "Hello World", 10
    len equ $ - message

section .text
_start:
    mov eax, 4          ; sys_write
    mov ebx, 1          ; stdout
    mov ecx, message    ; адрес сообщения
    mov edx, len        ; длина сообщения
    int 0x80            ; вызов ядра

    mov eax, 1          ; sys_exit
    mov ebx, 0
    int 0x80
